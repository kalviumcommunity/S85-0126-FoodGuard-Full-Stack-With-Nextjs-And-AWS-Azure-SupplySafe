{
  "buildCommand": "prisma generate && next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_NAME": "SupplySafe"
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "false"
    }
  }
}
