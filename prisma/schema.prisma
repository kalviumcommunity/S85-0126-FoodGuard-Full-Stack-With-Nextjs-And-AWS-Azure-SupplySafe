// This is a minimal Prisma schema for Docker testing
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

// Basic User model for testing
model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
